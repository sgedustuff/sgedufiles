import org.apache.log4j.Logger;
import org.apache.log4j.PropertyConfigurator;

public class TestLogging {

	
	
	public static void main(String[] args) {
		
		
		PropertyConfigurator.configure("D:\\Rajan\\Selenium-Practice\\16-logging\\src\\test\\resources\\properties\\log4j.properties");		
		Logger log = Logger.getLogger(TestLogging.class)
		
		
		log.info("1-info-message");
		log.debug("2-debug-message");
		log.error("3-error-message");
		
	}

}
